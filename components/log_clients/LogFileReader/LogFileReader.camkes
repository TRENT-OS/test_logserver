/**
 * @file
 * @brief   Log file reader used at the end of the test to dump logs from the
 *          files to the console.
 */

/** @cond SKIP_IMPORTS */
import <if_OS_Logger.camkes>;
/** @endcond */

/** @hideinitializer */
component LogFileReader {
    control;

    // interface to the log server
